@model IEnumerable<StoreTemplateCore.Entities.Product>
@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        @foreach (var product in Model)
        {
            <div class="col-md-4">
                @await Html.PartialAsync("ProductSummary",product)
            </div>
        }
    </div>
</div>
@*make view*@ 